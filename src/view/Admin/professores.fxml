               <children>
                  <Button fx:id="btnNaoRemove" layoutX="164.0" layoutY="91.0" mnemonicParsing="false" onAction="#carregaTelaProfessores" onMouseEntered="#acenderBtn" onMouseExited="#apagarBtn" prefHeight="25.0" prefWidth="90.0" style="-fx-background-color: #274D63;" text="NÃO" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnSimRemove" layoutX="23.0" layoutY="90.0" mnemonicParsing="false" onAction="#removerItems" onMouseEntered="#acenderBtn" onMouseExited="#apagarBtn" prefHeight="27.0" prefWidth="90.0" style="-fx-background-color: white; -fx-border-style: solid; -fx-border-radius: 3px; -fx-border-color: red;" text="SIM" textFill="RED">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Label alignment="CENTER" contentDisplay="TOP" layoutX="23.0" layoutY="34.0" prefHeight="21.0" prefWidth="231.0" text="Deseja realmente excluir os items?">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane fx:id="alertPane" layoutX="170.0" prefHeight="47.0" prefWidth="361.0" visible="false">
               <children>
                  <Label fx:id="lblErroRemove" layoutX="47.0" layoutY="6.0" text="Algum(s) Professores não puderam ser removidos!" textFill="#bc0f0f" />
                  <Label layoutX="26.0" layoutY="25.0" text="Professores que possuem turmas não podem ser exluidos!" textFill="#bc0f0f" />
               </children>
            </Pane>
         </children></Pane>
