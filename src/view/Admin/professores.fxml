<BorderPane fx:id="principalPane" onMouseClicked="#desativaAlert" prefHeight="530.0" prefWidth="797.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminController">
